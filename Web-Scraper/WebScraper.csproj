  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <RootNamespace>OneByteCaps.WebScraper</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <StartupObject>OneByteCaps.WebScraper.App</StartupObject>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
    <DocumentationFile>docs\</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Model\" />
